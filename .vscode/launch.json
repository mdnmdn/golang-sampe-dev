  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}/main.go",
      "env": {
        "PORT": "8050"
      },
      "args": []
    },
    {
      "name": "Remote",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "port": 2345,
      "host": "127.0.0.1",
      "remotePath":"/app",      

    }
  ]
}